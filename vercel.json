{
    "version": 2,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.3.1" }
    },
    "rewrites": [
        {
            "source": "/(css|js)/(.*)",
            "destination": "/public/$1/$2"
        },
        {
            "source": "/welcome",
            "destination": "/api/index.php"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ]
}
